{
  "name": "@nasapds/pds-wds-react",
  "version": "0.2.0-beta.0",
  "description": "PDS widget library for react and embeddable js",
  "main": "build/index.js",
  "author": "PDS",
  "homepage": "https://github.com/NASA-PDS/pds-wds-react/tree/embed-and-npm-builder",
  "scripts": {
    "embeddev": "node scripts/embeddev.js",
    "embedbuild": "node scripts/embedbuild.js",
    "npmbuild": "webpack --mode production",
    "build": "npm run embedbuild && npm run npmbuild",
    "demo": "node scripts/demo.js",
    "test": "node scripts/test.js",
    "coverage": "node scripts/test.js --coverage --watchAll=false",
    "postinstall": "node bin/postinstall",
    "prepare": "npm run build"
  },
  "keywords": [
    "starter",
    "React",
    "javascript",
    "embeddable",
    "react-widget",
    "pds"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.4.5",
    "@svgr/webpack": "6.5.0",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "3.0.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.10.4",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "4.0.0",
    "fs-extra": "8.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.0.0",
    "jest": "24.8.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.8.0",
    "jest-watch-typeahead": "0.3.1",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "resolve": "1.11.1",
    "sass-loader": "7.1.0",
    "semver": "6.1.2",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "url-loader": "2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "4.11.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1",
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "extract-text-webpack-plugin": "^3.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/lib/index.js",
      "!src/demo/index.js",
      "!src/utils/**",
      "!src/**/*.d.ts",
      "!**/tests/**"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/tests/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ],
      "transform-object-rest-spread",
      "@babel/plugin-transform-react-jsx"
    ]
  }
}
