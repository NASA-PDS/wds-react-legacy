{
  "name": "@nasapds/pds-wds-react",
  "version": "0.3.0",
  "description": "PDS widget library for react and embeddable js",
  "main": "build/index.js",
  "author": "PDS",
  "homepage": "https://github.com/NASA-PDS/pds-wds-react/tree/embed-and-npm-builder",
  "scripts": {
    "embeddev": "node scripts/embeddev.js",
    "embedbuild": "node scripts/embedbuild.js",
    "npmbuild": "webpack --mode production",
    "build": "npm run embedbuild && npm run npmbuild",
    "demo": "node scripts/demo.js",
    "test": "node scripts/test.js",
    "coverage": "node scripts/test.js --coverage --watchAll=false",
    "postinstall": "node bin/postinstall",
    "prepare": "npm run build"
  },
  "keywords": [
    "starter",
    "React",
    "javascript",
    "embeddable",
    "react-widget",
    "pds"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.25.4",
    "@fontsource/roboto": "^5.0.14",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@svgr/webpack": "8.1.0",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "9.1.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^10.0.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "7.1.2",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "eslint": "^9.9.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "6.2.0",
    "fs-extra": "11.2.0",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^3.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-resolve": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "mini-css-extract-plugin": "^2.9.1",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "pnp-webpack-plugin": "1.7.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "8.1.1",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "10.0.2",
    "postcss-safe-parser": "7.0.0",
    "react": ">=17.0.2 <19.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": ">=17.0.2 <19.0.0",
    "react-redux": ">=7.2.4 <10.0.0",
    "react-router-dom": "^6.26.1",
    "redux-saga": "^1.3.0",
    "redux-thunk": "^3.1.0",
    "resolve": "1.22.8",
    "sass-loader": "16.0.1",
    "semver": "7.6.3",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.10",
    "url-loader": "4.1.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-manifest-plugin": "5.0.0",
    "workbox-webpack-plugin": "7.1.0"
  },
  "peerDependencies": {
    "@fontsource/roboto": "^5.0.14",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "react": ">=17.0.2 <19.0.0",
    "react-dom": ">=17.0.2 <19.0.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/lib/index.js",
      "!src/demo/index.js",
      "!src/utils/**",
      "!src/**/*.d.ts",
      "!**/tests/**"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/tests/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ],
      "transform-object-rest-spread",
      "@babel/plugin-transform-react-jsx"
    ]
  }
}
